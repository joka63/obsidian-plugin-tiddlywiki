Markdown {
    block = para*
    para = bold | italic | strike | code | plain
    plain = ( ~( "**" | "`" | "_" | "~~" ) any)+
    bold = "**" 
    italic = "_" 
    strike = "~~" 
    code = "`" (~"`" any)* "`"
}

TiddlyWikiMarkdown <: Markdown {
    bold := "''" 
    italic := "//" 
    plain := ( ~( "''" | "`" | "//" | "~~" ) any)+
}
